{
  "expo": {
    "name": "easygo-business",
    "slug": "easygo-business",
    "version": "0.1.3",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.geottuse.serviceapp.business",
      "buildNumber": "0",
      "infoPlist": {
        "NSCameraUsageDescription": "EasyGO (Business) uses camera to allow you to capture a photo of your location/menu/product/service",
        "NSMicrophoneUsageDescription": ""
      }
    },
    "android": {
      "package": "com.geottuse.serviceapp.business"
    },
    "notification": {
      "icon": "./assets/notification.png"
    }
  }
}
