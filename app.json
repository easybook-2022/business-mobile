{
  "expo": {
    "name": "EasyGO (Services)",
    "slug": "easygo-services",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.geottuse.serviceapp.services",
      "buildNumber": "0",
      "infoPlist": {
        "NSCameraUsageDescription": "EasyGO (Services) uses camera to allow you to capture a photo of your location/menu/product/service"
      }
    },
    "android": {
      "package": "com.geottuse.serviceapp.services"
    },
    "notification": {
      "icon": "./assets/notification.png"
    }
  }
}
